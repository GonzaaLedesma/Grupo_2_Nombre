<!DOCTYPE html>
<html lang="es">
  <%- include('../partials/head') %>
  <body>
    <%- include('../partials/header') %>
    <main>
        <% if(locals.adminLogin) {%> 
          <div class="positionBoton">
            <button class="botonCreacion"><a href="/administrador/creacion">Crear evento <i class="fa-solid fa-upload"></i></a></button>
          </div>
          <% } %>
      <!-------------------------Segun el gusto----------- -->
      <% if(locals.userLogin) {%> 
        <h2 class="titulosCategoria">SEGUN TU GUSTO</h2>
        <div class="cajaCategoriaLogged">
          <% dataValuesArray.forEach(item => { %>
            <% products.filter(product => product.id == item.evento_id).forEach(product => { %>
              <div class="card">
                <img
                  src="/images/catalogo/<%= product.foto_evento%>"
                  class="card-img-top"
                  alt="ImgBand"
                />
                <div class="card-body">
                  <h5 class="card-title"><%= product.nombre_evento%></h5>
                  <p class="card-text"><%= product.sede%></p>
                  <p class="card-text"><%= product.ubicacion%></p>
                  <p class="card-text">Participacion: <%= product.participacion%></p>
                  <p class="card-text card-date"><%= product.fecha%></p>
                  <% if(product.id_categoria == 4){ %>
                    <p class="card-text">NO SE ENCUENTRA DISPONIBLE</p>
                  <% }else{ %>
                    <a href="/producto/detalles/<%= product.id%>" class="btn btn-primary"
                      >Ir a detalles</a>
                  <% } %>
                </div>
              </div>
            <% }) %>
          <% }) %>
        </div>

        <% } %>
      <!-------------------------PRIMERO----------- -->
      <h2 class="titulosCategoria">ACTUALES</h2>
      <div class="cajaCategoria">
        <% products.filter(product => product.id_categoria ==
        '1').forEach(product => { %>
        <div class="card">
          <img
            src="/images/catalogo/<%= product.foto_evento%>"
            class="card-img-top"
            alt="ImgBand"
          />
          <div class="card-body">
            <h5 class="card-title"><%= product.nombre_evento%></h5>
            <p class="card-text"><%= product.sede%></p>
            <p class="card-text"><%= product.ubicacion%></p>
            <p class="card-text">Participacion: <%= product.participacion%></p>
            <p class="card-text card-date"><%= product.fecha%></p>
            <a
              href="/producto/detalles/<%= product.id%>"
              class="btn btn-primary"
              >Ir a detalles</a
            >
          </div>
        </div>
        <% }) %>
      </div>
      <!-------------------------SEGUNDO----------- -->
      <h2 class="titulosCategoria">NUEVOS</h2>
      <div class="cajaCategoria">
        <% products.filter(product => product.id_categoria ==
        '2').forEach(product => { %>
