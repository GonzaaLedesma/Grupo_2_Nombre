            <h5 class="card-title"><%= product.nombre_evento%></h5>
            <p class="card-text"><%= product.sede%></p>
            <p class="card-text"><%= product.ubicacion%></p>
            <p class="card-text">Participacion: <%= product.participacion%></p>
            <p class="card-text card-date"><%= product.fecha%></p>
            <a
              href="/producto/detalles/<%= product.id%>"
              class="btn btn-primary"
              >Ir a detalles</a
            >
          </div>
        </div>
        <% }) %>
      </div>
      <!-------------------------TERCERO----------- -->
      <h2 class="titulosCategoria">PROXIMAMENTE</h2>
      <div class="cajaCategoria">
        <% products.filter(product => product.id_categoria ==
        '3').forEach(product => { %>
        <div class="card">
          <img
            src="/images/catalogo/<%= product.foto_evento%>"
            class="card-img-top"
